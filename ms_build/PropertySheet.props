    <boost_path>C:\local\boost_1_76_0</boost_path>
    <boost_lib_path>C:\local\boost_1_76_0\lib64-msvc-14.2</boost_lib_path>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="boost_path">
      <Value>$(boost_path)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="boost_lib_path">
      <Value>$(boost_lib_path)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>